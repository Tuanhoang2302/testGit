.body {
    width: 100%;
    height: 100vh;
    padding: 0px;
    border: 0px;
    margin: 0px;
}
.header{
    background-color: #5bbbe4;
    width: 100%;
    height: 97px;
    margin-top:0px;
}
.header .logo {
    
    background-size: auto 97px;
    background-position: -340px center;
    background-repeat: no-repeat;
    width: 1024px;
    height: 97px;
    position: relative;
    margin: auto;
}
.logo:hover{
    cursor: pointer;
}
@font-face {
    font-family: 'Muli-Italic-VariableFont_wght';
    src: url('fonts/Muli-Italic-VariableFont_wght.ttf') format('truetype');
}
@font-face {
    font-family: 'Jellee-Roman';
    src: url('fonts/Jellee-Roman.otf') format('opentype');
}
@font-face {
    font-family: 'Jellee-Roman';
    src: url('fonts/Jellee-Roman.ttf') format('truetype');
}

.menu {
    min-width: 500px;
    height: 50px;
    width: 100%;
    position: absolute;
    top: 97px;
    left: 0px;
    right: 0px;
    bottom: 0px;
    background-color: #eeeeee;
    display:flex;
   
}
    .menu .menu-logo {
        height: 100%;
        min-width: 200px;
        margin-left: 250px;
        background-image: url('assets/logodragon.png');
        background-repeat: no-repeat;
        background-size: 80%;
        background-position: center;
    }
    .menu .menu-logo:hover{
        cursor:pointer;
    }
    .menu .page {
        min-width: 97.1px;
        line-height: 47.1px;
        text-align: center;
        height: 100%;
        color: #707c81;
        letter-spacing: 0.6px;
        margin-left: 30px;
        font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;
        font-weight:bold;
        background-attachment: fixed;
    }
        .menu .page:hover {
         color: #e85969;
         cursor:pointer;
        }