.body {
    width: 100%;
    height: 100vh;
}

.header {
    background-color: #5bbbe4;
    width: 100%;
    height: 97px;
    position: absolute;
    top: 0px;
    right: 0px;
    left: 0px;
}

    .header .logo {
        border: 1px solid #000000;
        background-size: auto 97px;
        background-position: -340px center;
        background-repeat: no-repeat;
        width: 1024px;
        height: 97px;
        position: relative;
        margin: auto;
    }

.logo:hover {
    cursor: pointer;
}

.menu {
    background-color: #eee;
    width: 100%;
    height: 50px;
    position: absolute;
    top: 97px;
    right: 0px;
    left: 0px;
    display: flex;
}

    .menu .menu-page {
        width: 1024px;
        height: 100%;
        position: relative;
        margin: auto;
        display: flex;
    }

        .menu .menu-page .logo {
            width: 150px;
            height: 100%;
            background-image: url('assets/logodragon.png');
            background-repeat: no-repeat;
            background-size: 150px 10px;
            background-position: center;
            margin-left: 20px;
        }

.page {
    display: flex;
}

    .page .choosepage {
        width: 97px;
        height: 100%;
        line-height: 47.1px;
        font-family: 'Lucida Sans', 'Lucida Sans Regular', 'Lucida Grande', 'Lucida Sans Unicode', Geneva, Verdana, sans-serif;
        font-weight: bold;
        margin-left: 45px;
        color: #717f84;
        letter-spacing: 0.6px;
    }

.choosepage:hover {
    color: #e8677c;
    cursor: pointer;
}

.content {
    background-color: #ffffff;
    width: 100%;
    height: calc(100% - 150px);
    position: absolute;
    top: 147px;
    right: 0px;
    left: 0px;
}

    .content .page-content {
        width: 1024px;
        height: 100%;
        position: relative;
        margin: auto;
        border: 1px solid #5bbbe4;
    }

.page-content {
    width: 100%;
    height: calc(100% - 125px);
    background-color: lightpink;
    display: flex;
}

    .page-content .program-menu {
        width: 240px;
        background-color: #5bbbe4;
    }

.return {
    display: flex;
    width: auto;
    height: 24px;
    margin-left: 20px;
}

.program-menu .Back {
    font-family: 'Times New Roman', Times, serif,Arial, Helvetica, sans-serif;
    font-size: 18px;
    font-weight: 600;
    line-height: 100px;
    text-align: center;
    color: #717f84;
}

.table .program-menu-item {
    height: 60px;
}

.program-menu-item.number1, .number2, .number3 {
    font-family: 'Times New Roman', Times, serif,Arial;
    font-size: 20px;
    font-weight: bold;
    line-height: 46px;
    padding-left: 15px
}

.number1 {
    color: #ffffff;
    background-color: #00ad9b;
    border-top-left-radius: 5%;
    border-top-right-radius: 5%;
}
.number1:hover{
    cursor: pointer;
}
.number2 {
    background-color: #ffffff;
}

.number3:hover {
    color: #ff6171;
    cursor: pointer;
}

.number2:hover {
    color: #ff6171;
    cursor: pointer;
}
.number3 .secret-lab-icon {
    background-image: url('assets/icon3.png');
    width: 60px;
    height: 100%;
    margin-left:52px;
    background-position-y: 5px;
    background-repeat: no-repeat;
}
.number3 {
    background-color: #e9f9f8;
    border-bottom-left-radius: 5%;
    border-bottom-right-radius: 5%;
    display: flex;
}

.Back:hover {
    color: #e8677c;
}

.return:hover {
    cursor: pointer;
}

.page-content .table {
    width: 220px;
    height: 260px;
    position: absolute;
    top: 94px;
    margin-left: 20px;
}

.program-menu .icon-back {
    background-color: #ff6171;
    border-radius: 50%;
    width: 25px;
    height: 25px;
    background-image: url('assets/icon2.png');
    background-repeat: no-repeat;
    background-position: center;
    background-size: 7px;
    margin-right: 8px;
    /* position: absolute; */
    /* top: 10px; */
    margin-top: 37px;
}

.page-content .learn {
    width: calc(100% - 200px - 40px);
    border: 1px solid #000000;
}

.learn .title {
    font-family: Arial, Helvetica, sans-serif,'Times New Roman', Times, serif;
    font-weight: bold;
    font-size: 22px;
    color: #1a1a1a;
    width: 100%;
    letter-spacing: 0.6px;
    line-height: 98px;
    text-align: center;
    border: 1px solid #000000;
}

.main-content {
    width: 100%;
    position: absolute;
    top: 147px;
    display: flex;
}

    .main-content .lesson {
        display: inline-block;
        height: 100%;
        margin: 16px;
        width: 160px;
        border: 1px solid #b0d2a8;
    }

.lesson-1 {
}

.image-content {
    width: 160px;
    height: 160px;
    border: 4px solid #b0d2a8;
    border-radius: 10px;
    margin-top: -3.6px;
}

    .image-content:hover {
        cursor: pointer;
    }

.image-1 {
    background-repeat: no-repeat;
    background-position: center;
    background-image: url('assets/1.png');
}

.image-2 {
    background-repeat: no-repeat;
    background-position: center;
    background-image: url('assets/2.png');
}

.image-3 {
    background-repeat: no-repeat;
    background-position: center;
    background-image: url('assets/3.png');
}

.image-4 {
    background-repeat: no-repeat;
    background-position: center;
    background-image: url('assets/4.png');
}

.button {
    background-color: #b0d2a8;
    border-radius: 50%;
    width: 40px;
    height: 40px;
    margin-top: -178px;
    margin-left: 143px;
    background-image: url('assets/icon4.png');
    background-repeat: no-repeat;
    background-position: center;
}

.title-content {
    font-family: Arial, Helvetica, sans-serif,'Times New Roman', Times, serif;
    font-weight: 500;
    font-size: 14px;
    text-align: center;
    margin-top: 150px;
    color: #000000;
}

    .title-content:hover {
        color: #e8677c;
        cursor: pointer;
    }

.button:hover {
    cursor: pointer;
}
.breadcrumb{
    border: 1px solid #000000;
    display: flex;
    margin-left:  20px;
    margin-top: 40px;
}
.before{
    display: flex;
}
.button-before {
    width: 20px;
    height: 100%;
    border: 1px solid #00ad9b;
    background-image: url('assets/icon1.png');
    background-repeat: no-repeat;
    background-position: center;
    transform:rotate(-90deg);
}
.text{
    font-family:'Times New Roman', Times, serif;
    font-size: 15px;
    color: #000;
}
.now{
    font-weight:  bold;
}
